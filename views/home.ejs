<div class="w3-content w3-display-container w3-padding-32" style="max-width: 800px padding-left-20pz" >
    <div class="mysliders">
        <div class="mySlidesfade">
            
            <img src="css/imag/slide.png">
            
        </div>
        
        
        
        <div class="description"><br><br><strong class="cntent">Get Ready for Wce's most
            awaited megaevent!</strong><br><br>
            <p>ACM Club of WCE has came back again with its most awaited Hackathon</p>
            <br>
            <div class="cnt">
               <a href="https://www.wcehackathon.in/">Know More</a>  
            </div>
            <div class="image">
                
                <a href="https://www.wcehackathon.in/"> <img src="css/imag/arr.png" alt="" height="45px" width="53px"></a>
            </div>
            
        </div>
    </div>
    <div class="mysliders">
        <div class="mySlidesfade">
            
            <img src="css/imag/magglas.png">

        </div>
        
        
        
        <div class="description"><br><br><strong class="cntent">About our team.</strong><br><br>
            <p>To know more about us follow this article !</p>
            <br>
            <div class="cnt">
                <a href="/about">Know More</a>
            </div>
            <div class="image">
                
                <a href="/about"> <img src="css/imag/arr.png" alt="" height="45px" width="53px"></a>
            </div>
            
        </div>
    </div>
    <div class="mysliders">
        <div class="mySlidesfade">
            
            <img src="css/imag/wcelogo.png">
            
        </div>
        

        
        <div class="description"><br><br><strong class="cntent">About WCE Sangli</strong><br><br>
            <p><%=content%></p>
            <br>
            <div class="cnt">
                <a href="http://www.walchandsangli.ac.in/">Visit Site</a>
            </div>
            <div class="image">
                
                <a href="http://www.walchandsangli.ac.in/"> <img src="css/imag/arr.png" alt="" height="45px" width="53px"></a>
            </div>

        </div>
    </div>
    <div class="w3-center w3-container w3-section w3-large w3-text-black w3-display-bottommiddle" style="width:22%">
        <div class="w3-left w3-hover-text-purple" onclick="plusDivs(-1)">&#10094;</div>
        <div class="w3-right w3-hover-text-purple" onclick="plusDivs(1)">&#10095;</div>
        <span class="w3-badge demo w3-border w3-transparent w3-hover-aqua" onclick="currentDiv(1)"></span>
        <span class="w3-badge demo w3-border w3-transparent w3-hover-aqua" onclick="currentDiv(2)"></span>
        <span class="w3-badge demo w3-border w3-transparent w3-hover-aqua" onclick="currentDiv(3)"></span>
    </div>
</div>
<script>
    var slideIndex = 1;
    showDivs(slideIndex);
    
    function plusDivs(n) {
      showDivs(slideIndex += n);
    }
    
    function currentDiv(n) {
      showDivs(slideIndex = n);
    }
    
    function showDivs(n) {
      var i;
      var x = document.getElementsByClassName("mysliders");
      var dots = document.getElementsByClassName("demo");
      if (n > x.length) {slideIndex = 1}
      if (n < 1) {slideIndex = x.length}
      for (i = 0; i < x.length; i++) {
        x[i].style.display = "none";  
      }
      for (i = 0; i < dots.length; i++) {
        dots[i].className = dots[i].className.replace(" w3-white", "");
    }
      x[slideIndex-1].style.display = "block";  
      dots[slideIndex-1].className += " w3-white";
    }
    </script>

